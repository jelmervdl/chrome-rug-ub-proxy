{
  "name": "Use UB Proxy",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Opens a page through UB proxy of the University of Groningen. Note: you need a student or staff account.",
  "background": {
    "scripts": ["background.js"]
  },
  "page_action" :
  {
    "default_icon": "favicon.ico",
    "default_title": "Load via UB Proxy"
  },
  "permissions" : [
    "tabs"
  ],
  "icons" : {
    "48" : "logo-48.png",
    "128" : "logo-128.png"
  }
}
